<%-include("../partials/header.ejs") %>
<%-include("../partials/admin/navbar.ejs") %>
<%-include("../partials/admin/sidebar.ejs") %>


<main id="main" class="main">

  <div class="pagetitle">
    <h1>Verified Bookings</h1>
    <nav>
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="index.html">Home</a></li>
        <li class="breadcrumb-item">Bookings</li>
        <li class="breadcrumb-item active">Verified</li>
      </ol>
    </nav>
  </div><!-- End Page Title -->

  <section class="section">
    <div class="row-lg-6">
      <div class="col-lg-12">





        <div class="card">
          <div class="card-body">


            <!-- Table with hoverable rows -->
              <table class="table table-hover">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">Resident</th>
                    <th scope="col">CNIC</th>
                    <th scope="col">Location</th>
                    <th scope="col">Start Time</th>
                    <th scope="col">End Time</th>
                  </tr>
                </thead>

                <tbody>
                  <%  for (let i =0;i<bookings.length;i++){%>
                  <tr>
                    <th scope="row"><%=i+1%></th>
                    <td><%=bookings[i].resident.name%></td>
                    <td><%=bookings[i].resident.cnic  %></td>
                    <td><%=bookings[i].location  %></td>
                    <td><%=new Date(bookings[i].startTime).toLocaleDateString()  %></td>
                    <td><%=new Date(bookings[i].endTime).toLocaleDateString() %></td>
                  </tr>
                  <% } %>

                </tbody>
              </table>
              <!-- End Table with hoverable rows -->

          </div>
        </div>



      </div>
    </div>

    </div>
    </div>
  </section>

</main>





<%-include("../partials/footer.ejs") %>